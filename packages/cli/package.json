{
  "name": "@riseup/cli",
  "description": "Riseup CLI",
  "version": "0.21.0",
  "author": "Rafa Mel",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cli",
    "starter",
    "boilerplate",
    "riseup"
  ],
  "type": "module",
  "types": "./build/index.d.ts",
  "exports": "./build/index.js",
  "files": [
    "build/"
  ],
  "bin": {
    "riseup": "build/riseup.js"
  },
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "node": "../node_modules/.bin/kpo node --",
    "build": "../node_modules/.bin/kpo build --",
    "docs": "../node_modules/.bin/kpo docs --",
    "fix": "../node_modules/.bin/kpo fix --",
    "lint": "../node_modules/.bin/kpo lint --",
    "test": "../node_modules/.bin/kpo test --",
    "commit": "../node_modules/.bin/kpo commit --",
    "release": "../node_modules/.bin/kpo release --",
    "distribute": "../node_modules/.bin/kpo distribute --",
    "validate": "../node_modules/.bin/kpo validate --",
    "version": "../node_modules/.bin/kpo version --"
  },
  "homepage": "https://github.com/rafamel/riseup/tree/master/packages/cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/rafamel/riseup"
  },
  "bugs": {
    "url": "https://github.com/rafamel/riseup/issues"
  },
  "dependencies": {
    "cli-belt": "^2.0.0",
    "kpo": "^0.24.0"
  }
}
