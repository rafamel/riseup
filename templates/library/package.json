{
  "name": "library-template",
  "description": "Riseup library template",
  "version": "0.0.0",
  "author": "Rafa Mel",
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "keywords": [],
