    "url": "https://github.com/rafamel/library-template/issues"
  },
  "devDependencies": {
    "@jest/globals": "^28.1.0",
    "@riseup/library": "file:../../packages/library/tarball.tgz",
    "@riseup/tooling": "file:../../packages/tooling/tarball.tgz",
    "@riseup/universal": "file:../../packages/universal/tarball.tgz",
